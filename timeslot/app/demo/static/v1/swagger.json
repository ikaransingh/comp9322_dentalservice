{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "description": "This is a timeslot service API",
    "title": "ML timeslot service API ",
    "contact": {
      "email": "k.singh.1@student.unsw.edu.au"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "basePath": "/v1",
  "tags": [
    {
      "name": "default",
      "description": "Everything about timeslot service",
      "externalDocs": {
        "description": "Find out more",
        "url": "http://swagger.io"
      }
    }
  ],
  "paths": {
    "/timeslots": {
      "get": {
        "tags": [
          "default"
        ],
        "summary": "return all available timeslots",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "object",
              "properties": {
                "timeslot": {
                  "type": "string",
                  "example": [
                    "9",
                    "10",
                    "11",
                    "12"
                  ]
                }
              }
            }
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/booking": {
      "get": {
        "tags": [
          "default"
        ],
        "summary": "return booking id",
        "parameters": [
          {
            "in": "query",
            "name": "patientName",
            "description": "Enter patient name",
            "required": true,
            "type": "string"
          },
          {
            "in": "query",
            "name": "dentistName",
            "description": "Enter dentist name",
            "required": true,
            "type": "string"
          },
          {
            "in": "query",
            "name": "timeslot",
            "description": "Enter timeslot",
            "required": true,
            "type": "string"
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "object",
              "properties": {
                "b_id": {
                  "type": "string",
                  "example": "1000"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error"
          }
        }
      },
      "post": {
        "tags": [
          "default"
        ],
        "summary": "add new booking information",
        "parameters": [
          {
            "in": "body",
            "name": "booking",
            "schema": {
              "$ref": "#/definitions/booking"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "created"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/booking/{b_id}": {
      "delete": {
        "tags": [
          "default"
        ],
        "summary": "Cancel appointment by booking id",
        "description": "For valid response try integer IDs. Negative or non-integer values will generate API errors",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "b_id",
            "in": "path",
            "description": "ID of the appointment that needs to be deleted",
            "required": true,
            "type": "integer"
          }
        ],
        "responses": {
          "204": {
            "description": "delete successful operation"
          },
          "400": {
            "description": "Invalid ID supplied"
          }
        }
      }
    }
  },
  "definitions": {
    "booking": {
      "type": "object",
      "properties": {
        "patientName": {
          "type": "string",
          "example": "Krishna"
        },
        "dentistName": {
          "type": "string",
          "example": "Dr. Virdi"
        },
        "timeslot": {
          "type": "string",
          "example": "10"
        }
      }
    }
  },
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "http://swagger.io"
  }
}